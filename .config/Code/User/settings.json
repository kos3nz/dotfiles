  /* -------------------------------------------------------------------------- */
  /*                          Tailwind CSS IntelliSense                         */
  /* -------------------------------------------------------------------------- */
  "css.validate": false, // CSSのチェック機能をオフ
  "scss.validate": false, // CSSのチェック機能をオフ
  "editor.quickSuggestions": {
    "strings": true // JSXでもCSSクラスを補完できるように
  },
  "tailwindCSS.includeLanguages": {
    "javascript": "javascript",
    "javascriptreact": "javascript",
    "html": "HTML",
    "typescript": "javascript",
    "typescriptreact": "javascript"
  },
  "tailwindCSS.emmetCompletions": true, // emmetでTailwind CSSのクラスを使えるように
  "tailwindCSS.validate": true, // Tailwind CSSのDirectivesをLintする
  "tailwindCSS.experimental.classRegex": [
    // "className = ['\"`]([^('\"`)]*)",
    "tw`([^`]*)", // tw`...`
    "tw=\"([^\"]*)", // <div tw="..." />
    "tw={\"([^\"}]*)", // <div tw={"..."} />
    "tw\\.\\w+`([^`]*)", // tw.xxx`...`
    "tw\\(.*?\\)`([^`]*)" // tw(Component)`...`
  ], // 上記の形でもVSCodeが補完してくれる

  /* -------------------------------------------------------------------------- */
  /*                               Github Copilot                               */
  /* -------------------------------------------------------------------------- */
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false,
    "mdx": false
  },

  /* -------------------------------------------------------------------------- */
  /*                               Indent Rainbow                               */
  /* -------------------------------------------------------------------------- */
  "indentRainbow.colors": [
    "rgba(3, 4, 94,0.15)",
    "rgba(2, 62, 138,0.15)",
    "rgba(0, 119, 182, 0.15)",
    "rgba(0, 150, 199,0.15)",
    "rgba(0, 180, 216,0.15)",
    "rgba(72, 202, 228,0.15)",
    "rgba(144, 224, 239,0.15)"
  ],

  /* -------------------------------------------------------------------------- */
  /*                                     Git                                    */
  /* -------------------------------------------------------------------------- */
  "git.confirmSync": false,
  "diffEditor.wordWrap": "off",

  /* -------------------------------------------------------------------------- */
  /*                               Thunder Client                               */
  /* -------------------------------------------------------------------------- */
  "thunder-client.httpVersion": "HTTP/2",

  /* -------------------------------------------------------------------------- */
  /*                                 Error Lens                                 */
  /* -------------------------------------------------------------------------- */
  "errorLens.delay": 500,

  /* -------------------------------------------------------------------------- */
  /*                                   Drawio                                   */
  /* -------------------------------------------------------------------------- */
  "hediet.vscode-drawio.codeLinkActivated": false,
  "hediet.vscode-drawio.theme": "dark",

  /* -------------------------------------------------------------------------- */
  /*                                 Svg Preview                                */
  /* -------------------------------------------------------------------------- */
  "svgPreview.autoOpen": false,

  /* -------------------------------------------------------------------------- */
  /*                               Comment Divider                              */
  /* -------------------------------------------------------------------------- */
  "comment-divider.mainHeaderTransform": "titlecase", //先頭を大文字にする。
  "comment-divider.subheaderTransform": "titlecase", //先頭を大文字にする。

  /* -------------------------------------------------------------------------- */
  /*                                Multicommand                                */
  /* -------------------------------------------------------------------------- */
  "multiCommand.commands": [
    {
      "command": "multiCommand.down3Lines",
      "sequence": ["cursorDown", "cursorDown", "cursorDown"]
    },
    {
      "command": "multiCommand.swapChar",
      "interval": 30,
      "sequence": [
        "cursorLeftSelect",
        "editor.action.clipboardCutAction",
        "cursorRight",
        "editor.action.clipboardPasteAction"
      ]
    }
  ],

  /* -------------------------------------------------------------------------- */
  /*                                     Vim                                    */
  /* -------------------------------------------------------------------------- */
  "vim.leader": "<space>",
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.timeout": 10000,
  "vim.whichwrap": "h,l",

  // Plugins
  "vim.easymotion": true,
  "vim.camelCaseMotion.enable": true,
  "vim.replaceWithRegister": true,
  "vim.autoSwitchInputMethod.enable": true,
  "vim.autoSwitchInputMethod.defaultIM": "com.apple.keylayout.ABC",
  "vim.autoSwitchInputMethod.obtainIMCmd": "/usr/local/bin/im-select",
  "vim.autoSwitchInputMethod.switchIMCmd": "/usr/local/bin/im-select {im}",
  "vim.argumentObjectOpeningDelimiters": ["(", "[", "{"],
  "vim.argumentObjectClosingDelimiters": [")", "]", "}"],

  // Custom key mappings
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-e>": false,
    "<C-y>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    /* -------------------------------- Essential ------------------------------- */
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["<C-r>"],
      "commands": ["redo"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    /* --------------------------------- Sidebar -------------------------------- */
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "E"],
      "commands": ["workbench.action.toggleActivityBarVisibility"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["workbench.explorer.fileView.focus"]
    },
    /* --------------------------------- Buffer --------------------------------- */
    {
      "before": ["<leader>", "x"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "b", "x"],
      "commands": ["workbench.action.closeOtherEditors"]
    },
    {
      "before": ["<leader>", "b", "t"],
      "commands": ["workbench.action.files.newUntitledFile"]
    },
    /* ---------------------------------- File ---------------------------------- */
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "f", "a"],
      "commands": ["workbench.action.findInFiles"]
    },
    /* --------------------------------- Search --------------------------------- */
    {
      "before": ["<leader>", "h"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "s", "k"],
      "commands": ["workbench.action.showCommands"]
    },
    /* ----------------------------- Recent/session ----------------------------- */
    {
      "before": ["<leader>", "r", "b"],
      "commands": ["workbench.action.showAllEditorsByMostRecentlyUsed"]
    },
    /* ----------------------------------- Lsp ---------------------------------- */
    {
      "before": ["K"],
      "after": ["g", "h"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["<leader>", "l", "a"],
      "commands": ["editor.action.quickFix"]
    },
    /* ----------------------------- Cursor & Motion ---------------------------- */
    {
      // search character
      "before": ["f"],
      "after": ["<leader>", "<leader>", "s", "<char>"]
    },
    {
      // move to start of line
      "before": ["<leader>", "j"],
      "after": ["<leader>", "<leader>", "<leader>", "b", "d", "j", "k"]
    },
    // Camel Case Motion
    {
      "before": ["w"],
      "after": ["<leader>", "w"]
    },
    {
      "before": ["e"],
      "after": ["<leader>", "e"]
    },
    {
      "before": ["b"],
      "after": ["<leader>", "b"]
    },
    {
      "before": ["c", "m", "w"],
      "after": ["c", "i", "<leader>", "w"]
    },
    {
      "before": ["d", "m", "w"],
      "after": ["d", "i", "<leader>", "w"]
    },
    /* ------------------------------- Change Case ------------------------------ */
    {
      "before": ["g", "a"],
      "commands": ["extension.changeCase.commands"]
    },
    /* ----------------------------------- Ui ----------------------------------- */
    {
      "before": ["<leader>", "u", "m"],
      "commands": ["editor.action.toggleMinimap"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["<C-d>"],
      "commands": ["deleteRight"]
    }
  ],

  // ============================================= //
  /* Which key */
  "whichkey.sortOrder": "nonNumberFirst",
  "whichkey.bindingOverrides": [
    {
      "keys": "r",
      "name": "+Resume/Repeat",
      "type": "bindings",
      "bindings": []
    }
  ],

  /* -------------------------------------------------------------------------- */
  /*                                    Other                                   */
  /* -------------------------------------------------------------------------- */
  "redhat.telemetry.enabled": false,
  "editor.minimap.enabled": false
